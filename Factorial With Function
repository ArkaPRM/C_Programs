#include <stdio.h>
#include <conio.h>
int factorial (int a);
void main ()
{
  int n1 = 4, n2 = 5;
  printf ("Factorial of %d = %d", n1, factorial (n1));
  printf ("\nFactorial of %d = %d", n2, factorial (n2));
}

int
factorial (int a)
{
  int i, result = 1;
  for (i = 1; i <= a; i++)
    result = result * i;
  return result;
}
