#include <stdio.h>
#include <string.h>
struct ABC
    {
        int id, age;
        char name[20];
    };
int main ()
{
    struct ABC first;
    printf ("Enter employee ID: ");
    scanf ("%d",&first.id);
    printf ("Enter employee Name: ");
    scanf ("%s",&first.name);
    printf ("Enter employee Age: ");
    scanf ("%d",&first.age);
    printf ("Employee ID is %d, Employee name is %s, Employee age is %d",first.id,first.name,first.age);
}
